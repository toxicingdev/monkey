if game.PlaceId==155615604 then local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()local b=a.CreateLib("Prison Life","Sentinel")local c=b:NewTab("Main")local d=c:NewSection("Main")d:NewDropdown("Give Gun","Gives the localplayer a gun",{"M9","Remington 870","AK-47"},function(e)local f=game:GetService("Workspace")["Prison_ITEMS"].giver[e].ITEMPICKUP;local g=game:GetService("Workspace").Remote.ItemHandler;g:InvokeServer(f)end)d:NewDropdown("Gun Mod","Makes the gun op",{"M9","Remington 870","AK-47"},function(e)local h=nil;if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(e)then h=require(game:GetService("Players").LocalPlayer.Backpack[e].GunStates)elseif game:GetService("Players").LocalPlayer.Character:FindFirstChild(e)then h=require(game:GetService("Players").LocalPlayer.Character[e].GunStates)end;if h~=nil then h["MaxAmmo"]=math.huge;h["CurrentAmmo"]=math.huge;h["StoredAmmo"]=math.huge;h["FireRate"]=0.000001;h["Spread"]=0;h["Range"]=math.huge;h["Bullets"]=10;h["ReloadTime"]=0.000001;h["AutoFire"]=true end end)local i=b:NewTab("Player")local j=i:NewSection("Player")j:NewSlider("Walkspeed","Changes the walkspeed",250,16,function(e)game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=e end)j:NewSlider("Jumppower","Changes the jumppower",250,50,function(e)game.Players.LocalPlayer.Character.Humanoid.JumpPower=e end)elseif game.PlaceId==3956818381 then local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()local b=a.CreateLib("Ninja Legends","Sentinel")local c=b:NewTab("Main")local d=c:NewSection("Main")d:NewToggle("Auto Swing","Make your player autoswing",function(e)getgenv().autoswing=e;while true do if not getgenv().autoswing then return end;for k,e in pairs(game.Players.LocalPlayer.Backpack:GetChildren())do if e:FindFirstChild("ninjitsuGain")then game.Players.LocalPlayer.Character.Humanoid:EquipTool(e)break end end;local f="swingKatana"local g=game:GetService("Players").LocalPlayer.ninjaEvent;g:FireServer(f)wait(0.1)end end)d:NewToggle("Auto Sell","Makes your player autosell",function(e)getgenv().autosell=e;while true do if getgenv().autoswing==false then return end;game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;wait(0.1)game:GetService("Workspace").sellAreaCircles["sellAreaCircle16"].circleInner.CFrame=CFrame.new(0,0,0)wait(0.1)end end)d:NewButton("Unlock all islands","Unlocks all islands",function()local l=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;for k,e in pairs(game:GetService("Workspace").islandUnlockParts:GetChildren())do game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=e.CFrame;wait(0.1)end;wait(0.1)game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=l end)d:NewToggle("Auto buy all swords","Auto buys all swords",function(e)getgenv().buyswords=e;while true do if not getgenv().buyswords then return end;local f="buyAllSwords"local m="Inner Peace Island"local g=game:GetService("Players").LocalPlayer.ninjaEvent;g:FireServer(f,m)wait(0.5)end end)d:NewToggle("Auto buy all belts","Auto buys all belts",function(e)getgenv().buybelts=e;while true do if not getgenv().buybelts then return end;local f="buyAllBelts"local m="Inner Peace Island"local g=game:GetService("Players").LocalPlayer.ninjaEvent;g:FireServer(f,m)wait(0.5)end end)end
